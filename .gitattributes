
# try to avoid line ending issues ()
text=auto

*.asax   text eol=lf
*.bat    text eol=crlf
*.ccproj text eol=crlf  merge=union
*.cmd    text eol=crlf
*.cs     text eol=lf    diff=csharp
*.cscfg  text eol=lf
*.csdef  text eol=lf
*.cshtml text eol=lf
*.csproj text eol=lf  merge=union
*.css    text eol=lf
*.htm    text eol=lf    diff=html
*.html   text eol=lf    diff=html
*.js     text eol=lf
*.json   text eol=lf
*.less   text eol=lf
*.md     text eol=lf
*.nuspec text eol=lf
*.obproj text eol=lf
*.pdn    filter=lfs diff=lfs merge=lfs -text
*.ps1    text eol=lf
*.resx   text eol=lf    merge=union
*.sln    text eol=crlf  merge=union
*.smmx   text eol=lf
*.sh     text eol=lf
*.svg    text eol=lf
*.tt     text eol=lf
*.txt    text eol=lf
*.TXT    text eol=lf
*.vb     text eol=lf
*.wkt    text eol=lf
*.xml    text eol=lf
*.XML    text eol=lf
*.xsd    text eol=lf

*.css.map      text eol=lf
*.js.map       text eol=lf
*.min.map      text eol=lf
*.DotSettings  text eol=lf
*.pubxml       text eol=lf
*.snippet      text eol=lf
*.StyleCop     text eol=lf
*.targets      text eol=lf
*.testsettings text eol=lf


app.config          text eol=lf
App.config          text eol=lf
build.proj          text eol=lf
diagnostics.config  text eol=lf
package.config      text eol=lf
NuGet.Config        text eol=lf
web.config          text eol=lf
Web.config          text eol=lf
Web.*.config        text eol=lf


